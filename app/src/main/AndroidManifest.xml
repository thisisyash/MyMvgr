<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shaavy.mycollege.mvgr.mymvgr">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launchermvgr"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launchermvgr"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/mvgrlogo1" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/black" />

        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SendActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".PhoneAuthActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".CollegeId" />
        <activity android:name=".configure.Departments" />
        <activity android:name=".configure.Sections" />
        <activity android:name=".configure.Stream" />
        <activity android:name=".configure.Years" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".NoticeActivity" />
        <activity android:name=".ViewNotice" />
        <activity android:name=".AnnouncementActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".ViewFeedback" />
        <activity android:name=".AssignmentActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".FileActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".AttendanceActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".ErrorActivity" />
        <activity android:name=".FileDetailActivity" />
        <activity android:name=".ViewFileActivity" />
        <activity android:name=".TeamMvgr"></activity>
    </application>

</manifest>